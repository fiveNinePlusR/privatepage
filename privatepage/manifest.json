{

    "description": "Open the current tab in a private browser session",
    "manifest_version": 2,
    "name": "Private Page",
    "version": "0.1.0",
    "homepage_url": "https://github.com/fiveNinePlusR/privatepage",
    "icons": {
        "48": "icons/Privacy-16.png"
    },

    "permissions": [
      "tabs"
    ],

  "background": {
      "scripts": ["background.js"]
  },

  "browser_action": {
      "default_icon": "icons/privatebrowsing-mask-short@2x.png"
  }
}
